<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <GeneratedAssemblyInfoFile>Properties\assemblyinfo.cs</GeneratedAssemblyInfoFile>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="Aserto.Client.Grpc" Version="0.8.0" />
    <PackageReference Include="dotenv.net" Version="3.1.1" />
    <PackageReference Include="Grpc.Net.Client" Version="2.42.0" />
    <PackageReference Include="Okta.Sdk" Version="5.4.1" />
    <PackageReference Include="System.CommandLine" Version="2.0.0-beta3.22114.1" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Okta.Sdk" />
  </ItemGroup>

</Project>
